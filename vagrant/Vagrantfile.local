# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|

  # Use vagrant-winnfsd.
  #   vagrant plugin install vagrant-winnfsd
  config.vm.synced_folder "C:/R", "/r", type: "smb", smb_username: "<your name>", smb_password: "<your password>"

  config.vm.provider "virtualbox" do |vb|
    vb.gui = false
    vb.memory = "500"
  end

end

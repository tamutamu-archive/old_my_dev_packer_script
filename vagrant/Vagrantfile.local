# -*- mode: ruby -*-
# vi: set ft=ruby :

Vagrant.configure("2") do |config|

  # Use vagrant-winnfsd.
  #   vagrant plugin install vagrant-winnfsd
  config.vm.synced_folder "C:/R", "/r", nfs: true, linux__nfs_options: ['rw','no_subtree_check','all_squash','async']

  config.vm.provider "virtualbox" do |vb|
    vb.gui = false
    vb.memory = "500"
  end

end
